{
  "plugins": ["compat", "github"],
  "extends": [
    "next/core-web-vitals",
    "plugin:compat/recommended",
    "plugin:github/react"
  ],
  "rules": {
    "no-console": "warn",
    "no-duplicate-imports": ["error", { "includeExports": true }],
    "sort-imports": "error",
    "no-unused-vars": "warn"
  },
  "env": {
    "browser": true
  }
}
